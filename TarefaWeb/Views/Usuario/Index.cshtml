
@{
    ViewBag.Title = "Index";
}

<table class="table table-striped">
    @foreach (var u in Model)
    {
    <tr>
        <td>@u.UsuarioId</td>
        <td>@u.Nome</td>
        <td>@u.Email</td>
        <td>@u.Senha</td>
        <td>@u.Receita</td>
        <td>@Html.ActionLink("Editar", "Update", new { id = u.UsuarioId })</td>
        <td>
            <a href="/Usuario/Delete/@u.UsuarioId">Apagar</a>
        </td>
    </tr>
    }
</table>
<a href="/Usuario/Create">Adicionar</a>

